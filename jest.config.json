{
	"clearMocks": true,
	"collectCoverage": true,
	"collectCoverageFrom": [
		"**/*.js",
		"!**/test/**"
	],
	"coveragePathIgnorePatterns": [
		"/node_modules/"
	],
	"coverageThreshold": {
		"./packages/**/*.js": {
			"branches": 100,
			"functions": 100,
			"lines": 100,
			"statements": 100
		}
	},
	"projects": [
		"<rootDir>/examples/*",
		"<rootDir>/packages/*"
	],
	"testEnvironment": "node"
}