{
	"env": {
		"jest": true
	},
	"extends": ["@financial-times/eslint-config-next"],
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"ecmaVersion": 2020,
		"requireConfigFile": false,
		"sourceType": "script"
	},
	"plugins": [
		"prettier"
	],
	"rules": {
		"prettier/prettier": "error",
		"quotes": 0,
		"space-before-function-paren": 0
	}
}
